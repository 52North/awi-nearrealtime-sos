<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class mutable="false" name="org.n52.sensorweb.awi.data.entities.Data" optimistic-lock="version" schema="public" table="data">
    <id column="data_id" name="id" type="int"/>
    <property column="date" name="date" type="org.jadira.usertype.dateandtime.joda.PersistentDateTime"/>
    <property column="value" name="value" type="double"/>
    <many-to-one class="org.n52.sensorweb.awi.data.entities.Sensor" column="sensor_id" fetch="select" name="sensor"/>
    <property column="level" name="level" type="java.lang.Integer"/>
  </class>
</hibernate-mapping>
