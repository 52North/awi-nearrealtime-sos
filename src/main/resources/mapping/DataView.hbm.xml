<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class mutable="false" name="org.n52.sensorweb.awi.data.entities.DataView" optimistic-lock="version" schema="public" table="dataview">
        <id column="data_id" name="id" type="int"/>
        <property column="date" name="time" type="org.jadira.usertype.dateandtime.joda.PersistentDateTime"/>
        <property column="value" name="value" type="double"/>
        <property column="latitude" name="latitude" type="double"/>
        <property column="longitude" name="longitude" type="double"/>
        <property column="code" name="code" type="string"/>
        <many-to-one class="org.n52.sensorweb.awi.data.entities.Sensor" column="sensor_id" fetch="join" name="sensor"/>
    </class>
</hibernate-mapping>
